#include<stdio.h>
main(){

int A, B, C;

printf("Digite o valor de A");
scanf("%d", &A);

printf("Digite o valor de B");
scanf("%d", &B);

printf("Digite o valor de C");
scanf("%d", &C);

if(A>B && B>C){
printf("A ordem correta do maior para o menor e: A, B, C");}

else if(B>A && A>C){
printf("A ordem correta do maior para o menor e: B, A, C");}

else if(B>C && C>A){
printf("A ordem correta do maior para o menor e: B, C, A");}

else if(C>A && A>B){
printf("A ordem correta do maior para o menor e: C, A, B");}

}
